				mGetString MACRO prompt, bufferSize, bufferAddress, bytesEntered

				pushad
				mov EDX, prompt
				call WriteString
				mov EDX, bufferAddress
				mov ECX, bufferSize
				call ReadString
				mov EBX, bytesEntered
				mov [EBX], EAX

				popad
				ENDM

